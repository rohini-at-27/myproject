{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "MobileNumber": {
      "type": "biginteger",
      "unique": true,
      "required": true
    },
    "Age": {
      "type": "integer",
      "default": 200,
      "unique": false,
      "required": true
    },
    "bought_books": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::bookmodel.bookmodel",
      "inversedBy": "customers"
    },
    "sold_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::employee.employee",
      "inversedBy": "sold_to"
    },
    "order_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "mappedBy": "customer"
    }
  }
}
