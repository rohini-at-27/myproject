{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "MobileNumber": {
      "type": "biginteger",
      "unique": true,
      "required": true
    },
    "Age": {
      "type": "integer",
      "default": 100,
      "unique": false,
      "required": true
    },
    "Preference": {
      "type": "enumeration",
      "enum": [
        "fiction",
        "science fiction",
        "Thriller",
        "History",
        "Adventure",
        "Biography"
      ],
      "required": true
    }
  }
}
