{
  "kind": "collectionType",
  "collectionName": "bookmodels",
  "info": {
    "singularName": "bookmodel",
    "pluralName": "bookmodels",
    "displayName": "Bookmodel",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "BookName": {
      "type": "string",
      "required": true
    },
    "Language": {
      "type": "enumeration",
      "enum": [
        "English",
        "spanish",
        "italy",
        "tamil"
      ],
      "required": true
    },
    "AuthorName": {
      "type": "string",
      "required": true
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "Ratings": {
      "type": "integer",
      "required": true,
      "max": 5
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "bookmodel"
    },
    "customers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::customer.customer",
      "mappedBy": "bought_books"
    }
  }
}
