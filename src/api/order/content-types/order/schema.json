{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "MobileNumber": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "Sold_by": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "Order_Id": {
      "type": "biginteger"
    },
    "Expected_delivery_date": {
      "type": "date",
      "unique": false,
      "required": true
    },
    "Order_Status": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "Delivery_Partner": {
      "type": "enumeration",
      "enum": [
        "DHL",
        "bluedart",
        "Ekart"
      ],
      "required": true
    },
    "Address": {
      "type": "text",
      "required": true
    },
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer.customer",
      "inversedBy": "order_detail"
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "order"
    }
  }
}
